<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="learning-automation" verbose="1">
    <listeners>
        <listener class-name="testautomation.listener.TestListener"/>
    </listeners>
    <test name="post-tests">
        <classes>
            <class name="FirstTest">
                <methods>
                    <include name="test1FirstTest"/>
                    <include name="test2FirstTest"/>
                </methods>
            </class>

        </classes>
    </test>
</suite>